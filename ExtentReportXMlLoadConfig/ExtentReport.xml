<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    
    
    <!-- report theme -->
    <!-- standard, dark -->
    <theme>standard</theme>
  
    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and style sheets -->
    <!-- defaults to http's -->
    <protocol>https</protocol>
    
     
    
    <!-- title of the document -->
    <documentTitle>ExtentReports 2</documentTitle>
    
    <!-- report name - displayed at top-nav -->
    <reportName>ebay Report</reportName>
    
    <!-- report headline - displayed at top-nav, after reportHeadline -->
    <reportHeadline>Test Execution Done For ebay webpage </reportHeadline>
    
    <!-- global date format override -->
    <!-- defaults to yyyy-MM-dd -->
    <dateFormat>yyyy-MM-dd</dateFormat>
    
    <!-- global time format override -->
    <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
    
    
    
    !-- settings to enable/disable views -->
<enableCategoryView>true</enableCategoryView>
<enableAuthorView>true</enableAuthorView>
<enableExceptionView>true</enableExceptionView>
<enableTestRunnerLogsView>true</enableTestRunnerLogsView>
    
    
    
    <!-- custom javascript -->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
          
        });
      ]]>
    </scripts>
    
    <!-- custom styles -->
    <styles>
      <![CDATA[
        
      ]]>
    </styles>
  </configuration>
</extentreports>
